algoritmo "consumogasolina"
// Autor :Guilherme
// Data : 30/03/2023
// Professora: cintia  pinho
var
//entrada
quilo,cons ,preco,media,valorf: real

inicio
//processamento
escreval ("Digite quilo")
leia (quilo)
escreval ("Digite consumo")
leia (cons)
escreval ("Digite preço da gasolina")
leia (preco)

//processamento
media <- quilo/cons
valorf <- media*preco

//saída

escreval ("Seu gasto é de: ", valorf)

fimalgoritmo
